<?xml version='1.0' encoding='ISO-8859-1'?>
<fix8>
	<session name="TEX1"
				role="acceptor"
				fix_version="4400"
				active="true"
				heartbeat_interval="5"
				logname="./run/myfix_server.log" protocol_logname="./run/myfix_server_protocol.log"
				ip="127.0.0.1" port="11001"
				sender_comp_id="TEX_DLD"
				persist="bdb0" />

	<persist name="bdb0"
				type="bdb"
				dir="./run"
				db="myfix_server.db"/>

	<persist name="mem0"
				type="mem"/>
</fix8>
